* 
    margin 0
    padding 0

[draggable="true"] 
    user-select none
    -moz-user-select none
    -webkit-user-select none
    -ms-user-select none

.none
    display none

.block
    display block
    color #fff

.container 
    background-color #232323
    width 100%
    height 100%
    position absolute
    overflow hidden

.lienzo 
    background-color #fff
    width 900px
    height 500px
    margin 5px 0px 0px 15px
    position absolute
    overflow hidden

.panel 
    background-color #333
    width 415px
    margin 5px 15px 0px 0px
    height 500px
    position absolute
    right 0px

.contenedor_panel
    width 415px
    height 500px
    position absolute

.panel_interno
    width 355px
    height 500px
    position absolute
.time_line 
    background-color #333
    position absolute
    bottom 0px
    left 50%
    transform translate(-50%, 0%)
    width 98%
    height 140px

.img 
    position absolute
    overflow hidden


.img_watch 
    position absolute
    background rgba(60, 255, 0, 0.486)

.img_interna 
    width 100%
    height 100%
    position absolute

.name_attribute 
    color #fff
    font-family Verdana, Geneva, Tahoma, sans-serif
    margin 0px 0px
    text-align center
    -webkit-user-select none
    -moz-user-select none
    -khtml-user-select none
    -ms-user-select none

.input 
    background-color #fff
    border none
    padding 1px 10px
    border-radius 5px
    margin 10px
    width 50px
    right 0px

.table 
    background rgba(0, 0, 0, 0.329)
    border solid 2px rgba(0, 0, 0, 0.37)
    margin 3px
    float right
    text-align center


label 
    padding 5px 10px
    background #212121
    font-family Verdana, Geneva, Tahoma, sans-serif
    display table
    cursor pointer
    border-radius 50px
    // margin 10px 0px 5px 40px
    position absolute 
    bottom 10px
    left 10px
    color rgb(255, 255, 255)

input[type="file"] 
    display none

.emparejar 
    color #fff
    background-color #00000071
    position absolute
    padding 3px
    margin-top -10px
    margin-left 0px
    cursor pointer
    -webkit-user-select none
    -moz-user-select none
    -khtml-user-select none
    -ms-user-select none

.emparejar_activo 
    color #379c1e
    background-color #00000071
    position absolute
    padding 3px

    margin-top -10px
    margin-left 0px
    cursor pointer
    -webkit-user-select none
    -moz-user-select none
    -khtml-user-select none
    -ms-user-select none

.alto_1,
.alto_2,
.ancho_1,
.ancho_2 
    width 10px
    height 10px
    background-color #fff
    border-radius 50%
    border solid 1px #000
    position absolute

.alto_1 
    cursor move

.alto_2 
    cursor move

.ancho_1 
    cursor move

.ancho_2 
    cursor move

.imagenes_puntos
    background-color red

.columna
 width 45px
 height 100%
 background #2d2d2d
 position absolute
 right 0px

.btn_off
    color #ccc

ul
    list-style none
    font-size 20px
    li
        margin 15px 0px 10px 15px
        cursor pointer

.red_icon
    color #ff3232

.btn_on
    color #fff